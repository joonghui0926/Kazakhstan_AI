import time
from maix import GPIO
from fpioa_manager import fm

fm.register(33, fm.fpioa.GPIO0, force=True)
fm.register(24, fm.fpioa.GPIO1, force=True)
fm.register(32, fm.fpioa.GPIO2, force=True)

camera_button_right = GPIO(//)   # Set the GPIO0 pin to input mode
camera_button_center = GPIO(//)  # Set the GPIO1 pin to input mode
camera_button_left = GPIO(//)    # Set the GPIO2 pin to input mode

while True:
    if camera_button_right.value() == 0:
        print("right button")
        time.sleep(0.1)

    //

# If the camera_button is pressed, print "center button" and wait for 0.1 seconds
# If the camera_button_left is pressed, print "left button" and wait for 0.1 seconds
